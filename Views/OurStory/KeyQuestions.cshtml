
@{
    ViewBag.Title = "KeyQuestions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br /> <br /><br />
<div class="container">
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <center><h2 style="color:coral">Key Questions before we tell you about Our story </h2></center>
            <br />
            <hr />
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <br />
            <form>
                <div class="form-group">
                    <label for="Question1"><h3>Currently, where would you go for information on community colleges and universities in illinois ?</h3></label>
                    <input type="text" class="form-control form-control-lg" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Answer here">
                </div>

                <div class="form-group">
                    <label for="Question2"><h3>What do you want to know about about your institution or a community college or university in your community ?</h3></label>
                    <textarea class="form-control" id="Quest2" placeholder="Answer here" rows="3"></textarea>
                </div>
                <br />
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
        </div>
    </div>

